<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_JMO_Controller</servlet-name>
        <servlet-class>controller.Add_JMO_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_Police_Controller</servlet-name>
        <servlet-class>controller.Add_Police_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_Registrar</servlet-name>
        <servlet-class>controller.Add_Registrar_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Postmortem_controller</servlet-name>
        <servlet-class>controller.Postmortem_controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PM_Report</servlet-name>
        <servlet-class>controller.PM_Report</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteJMO</servlet-name>
        <servlet-class>controller.DeleteJMO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteJMOController</servlet-name>
        <servlet-class>controller.DeleteJMOController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Test1</servlet-name>
        <servlet-class>controller.Test1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MLRController</servlet-name>
        <servlet-class>controller.MLRController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/validate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_JMO_Controller</servlet-name>
        <url-pattern>/add_jmo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_Police_Controller</servlet-name>
        <url-pattern>/add_police</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_Registrar</servlet-name>
        <url-pattern>/add_registrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Postmortem_controller</servlet-name>
        <url-pattern>/Postmortem_controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PM_Report</servlet-name>
        <url-pattern>/PM_Report</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteJMO</servlet-name>
        <url-pattern>/DeleteJMO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteJMOController</servlet-name>
        <url-pattern>/DeleteJMOController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Test1</servlet-name>
        <url-pattern>/Test1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MLRController</servlet-name>
        <url-pattern>/MLRController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
